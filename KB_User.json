{
"BaseModMode": {
    "CapsLock":       { "Modifiers": ["RControl"]  },
    "LControl":       { "Modifiers": ["LControl"]  },
    "LShift":         { "Modifiers": ["LShift"]    },
    "LAlt":           { "Modifiers": ["LAlt"]      },
    "LWin":           { "Modifiers": ["LWin"]      },
    "RControl":       { "Modifiers": ["RControl"]  },
    "RShift":         { "Modifiers": ["RShift"]    },
    "RAlt":           { "Modifiers": ["RAlt"]      },

    "\\":             { "Action": "{Tab}"          }
},

"DefaultMode": {
    "`":      { "SetMode": "WinNMode"     },

    "'":      { "SetMode": "RNavMode"     },
    ",":      { "SetMode": "RNavMode"     },
    ".":      { "SetMode": "RNavMode"     },
    "p":      { "SetMode": "RNavMode"     },

    "a":      { "SetMode": "RModMode"     },
    "o":      { "SetMode": "RModMode"     },
    "e":      { "SetMode": "RModMode"     },
    "u":      { "SetMode": "RNavMode"     },
    "i":      { "SetMode": "RModMode"     },

    "j":      { "SetMode": "RNumPadMode"  },

    "g":      { "SetMode": "LFuncMode"    },
    "c":      { "SetMode": "LFuncMode"    },
    "r":      { "SetMode": "LFuncMode"    },
    "l":      { "SetMode": "LFuncMode"    },

    "d":      { "SetMode": "LModMode"     },
    "h":      { "SetMode": "LFuncMode"    },
    "t":      { "SetMode": "LModMode"     },
    "n":      { "SetMode": "LModMode"     },
    "s":      { "SetMode": "LModMode"     },
    "-":      { "SetMode": "LModMode"     }
},

"WinNMode": {
    "`":     { "Modifiers": ["RControl", "LWin"]   }
},

"RNavMode": {
    "'":    { "Modifiers": ["LShift"]    },
    ",":    { "Modifiers": ["LAlt"]      },
    ".":    { "Modifiers": ["LControl"]  },
    "p":    { "Modifiers": ["LWin"]      },

    "a":    { "Modifiers": ["LShift"]    },
    "o":    { "Modifiers": ["LAlt"]      },
    "e":    { "Modifiers": ["LControl"]  },
    "u":    {   "SetMode":  "RNavMode"   },
    "i":    { "Modifiers": ["LWin"]      },

    "7":    { "Action": "{f7}"           },
    "8":    { "Action": "{f8}"           },
    "9":    { "Action": "{f9}"           },
    "0":    { "Action": "{f10}"          },
    "[":    { "Action": "{f11}"          },
    "]":    { "Action": "{f12}"          },

    "f":    { "Action": "{Escape}"       },
    "g":    { "Action": "{Home}"         },
    "c":    { "Action": "{PgUp}"         },
    "r":    { "Action": "{PgDn}"         },
    "l":    { "Action": "{End}"          },
    "/":    { "Action": "{Enter}"        },
    "=":    { "Action": "{AppsKey}"      },

    "d":    { "Action": "{Backspace}"    },
    "h":    { "Action": "{Left}"         },
    "t":    { "Action": "{Up}"           },
    "n":    { "Action": "{Down}"         },
    "s":    { "Action": "{Right}"        },
    "-":    { "Action": "{Delete}"       },

    "b":    { "Action": "x"              }
},

"RModMode": {
    "a":    { "Modifiers": ["LShift"]    },
    "o":    { "Modifiers": ["LAlt"]      },
    "e":    { "Modifiers": ["LControl"]  },
    "u":    {   "SetMode":  "RNavMode"   },
    "i":    { "Modifiers": ["LWin"]      }
},

"RNumPadMode": {
    "j":    { "SetMode": "RNumPadMode"     },

    "7":    {  "Action": "{Numpad7}"       },
    "8":    {  "Action": "{Numpad8}"       },
    "9":    {  "Action": "{Numpad9}"       },
    "0":    {  "Action": "{NumpadMult}"    },
    "[":    {  "Action": "("               },
    "]":    {  "Action": ")"               },

    "f":    {  "Action": "{Escape}"        },
    "g":    {  "Action": "{Numpad4}"       },
    "c":    {  "Action": "{Numpad5}"       },
    "r":    {  "Action": "{Numpad6}"       },
    "l":    {  "Action": "{NumpadSub}"     },
    "/":    {  "Action": "{NumpadEnter}"   },

    "d":    {  "Action": "{Backspace}"     },
    "h":    {  "Action": "{Numpad1}"       },
    "t":    {  "Action": "{Numpad2}"       },
    "n":    {  "Action": "{Numpad3}"       },
    "s":    {  "Action": "{NumpadAdd}"     },
    "-":    {  "Action": "{Delete}"        },

    "b":    {  "Action": "{:}"             },
    "m":    {  "Action": "{Numpad0}"       },
    "w":    {  "Action": "{^}"             },
    "v":    {  "Action": "{NumpadDot}"     },
    "z":    {  "Action": "{NumpadDiv}"     }
},


"LFuncMode": {
    "g":     { "Modifiers": ["LWin"]               },
    "c":     { "Modifiers": ["RControl"]           },
    "r":     { "Modifiers": ["RAlt"]               },
    "l":     { "Modifiers": ["RShift"]             },

    "d":     { "Modifiers": ["LWin"]               },
    "h":     {   "SetMode":  "LFuncMode"           },
    "t":     { "Modifiers": ["RControl"]           },
    "n":     { "Modifiers": ["RAlt"]               },
    "s":     { "Modifiers": ["RShift"]             },

    "1":     { "Action": "{f1}"                    },
    "2":     { "Action": "{f2}"                    },
    "3":     { "Action": "{f3}"                    },
    "4":     { "Action": "{f4}"                    },
    "5":     { "Action": "{f5}"                    },
    "6":     { "Action": "{f6}"                    }
},

"LModMode": {
    "d":     { "Modifiers": ["LWin"]               },
    "h":     {   "SetMode":  "LFuncMode"           },
    "t":     { "Modifiers": ["RControl"]           },
    "n":     { "Modifiers": ["RAlt"]               },
    "s":     { "Modifiers": ["RShift"]             },
    "-":     { "Modifiers": ["RControl", "LWin"]   }
}


}
