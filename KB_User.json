{
"DefaultMode": {
    "`":      { "SetMode": "TildeMode"    },

    "q":      { "SetMode": "RNavMode"     },
    "w":      { "SetMode": "RNavMode"     },
    "e":      { "SetMode": "RNavMode"     },
    "r":      { "SetMode": "RNavMode"     },
    "t":      { "SetMode": "RNavMode"     },

    "a":      { "SetMode": "RModMode"     },
    "s":      { "SetMode": "RModMode"     },
    "d":      { "SetMode": "RModMode"     },
    "f":      { "SetMode": "RNavMode"     },
    "g":      { "SetMode": "RModMode"     },

    "c":      { "SetMode": "RNumPadMode"  },

    "y":      { "SetMode": "LFuncMode"    },
    "u":      { "SetMode": "LFuncMode"    },
    "i":      { "SetMode": "LFuncMode"    },
    "o":      { "SetMode": "LFuncMode"    },
    "p":      { "SetMode": "LFuncMode"    },

    "h":      { "SetMode": "LModMode"     },
    "j":      { "SetMode": "LFuncMode"    },
    "k":      { "SetMode": "LModMode"     },
    "l":      { "SetMode": "LModMode"     },
    ";":      { "SetMode": "LModMode"     },

    "'":      { "SetMode": "RPinkyMode"   }
},

"Global": {

},

"TildeMode": {
    "`":       { "SetMode":  "TildeMode"     },
    "1":       { "Action": "{F1}"            },
    "2":       { "Action": "{F2}"            },
    "3":       { "Action": "{F3}"            },
    "4":       { "Action": "{F4}"            },
    "5":       { "Action": "{F5}"            },
    "6":       { "Action": "{F6}"            }
},

"RNavMode": {
    "q":    { "Modifiers": ["LShift"]    },
    "w":    { "Modifiers": ["LAlt"]      },
    "e":    { "Modifiers": ["LControl"]  },
    "r":    { "Modifiers": ["LWin"]      },
    "t":    {   "SetMode":  "RNavMode"   },

    "a":    { "Modifiers": ["LShift"]    },
    "s":    { "Modifiers": ["LAlt"]      },
    "d":    { "Modifiers": ["LControl"]  },
    "f":    {   "SetMode":  "RNavMode"   },
    "g":    { "Modifiers": ["LWin"]      },

    "7":    { "Action": "{f7}"           },
    "8":    { "Action": "{f8}"           },
    "9":    { "Action": "{f9}"           },
    "0":    { "Action": "{f10}"          },
    "-":    { "Action": "{f11}"          },
    "=":    { "Action": "{f12}"          },

    "y":    { "Action": "{Escape}"       },
    "u":    { "Action": "{Home}"         },
    "i":    { "Action": "{PgUp}"         },
    "o":    { "Action": "{PgDn}"         },
    "p":    { "Action": "{End}"          },
    "[":    { "Action": "{Enter}"        },
    "]":    { "Action": "{AppsKey}"      },
    "\\":   { "Action": "{Tab}"          },

    "h":    { "Action": "{Backspace}"    },
    "j":    { "Action": "{Left}"         },
    "k":    { "Action": "{Up}"           },
    "l":    { "Action": "{Down}"         },
    ";":    { "Action": "{Right}"        },
    "'":    { "Action": "{Delete}"       },

    "n":    { "Action": "x"              }
},

"RModMode": {
    "a":    { "Modifiers": ["LShift"]    },
    "s":    { "Modifiers": ["LAlt"]      },
    "d":    { "Modifiers": ["LControl"]  },
    "f":    {   "SetMode":  "RNavMode"   },
    "g":    { "Modifiers": ["LWin"]      }
},

"RNumPadMode": {
    "c":    { "SetMode": "RNumPadMode"     },

    "7":    {  "Action": "{Numpad7}"       },
    "8":    {  "Action": "{Numpad8}"       },
    "9":    {  "Action": "{Numpad9}"       },
    "0":    {  "Action": "{NumpadMult}"    },
    "-":    {  "Action": "("               },
    "=":    {  "Action": ")"               },

    "y":    {  "Action": "{Escape}"        },
    "u":    {  "Action": "{Numpad4}"       },
    "i":    {  "Action": "{Numpad5}"       },
    "o":    {  "Action": "{Numpad6}"       },
    "p":    {  "Action": "{NumpadSub}"     },
    "[":    {  "Action": "{NumpadEnter}"   },

    "h":    {  "Action": "{Backspace}"     },
    "j":    {  "Action": "{Numpad1}"       },
    "k":    {  "Action": "{Numpad2}"       },
    "l":    {  "Action": "{Numpad3}"       },
    ";":    {  "Action": "{NumpadAdd}"     },
    "'":    {  "Action": "{Delete}"        },

    "n":    {  "Action": "{:}"             },
    "m":    {  "Action": "{Numpad0}"       },
    ",":    {  "Action": "{^}"             },
    ".":    {  "Action": "{NumpadDot}"     },
    "/":    {  "Action": "{NumpadDiv}"     }
},


"LFuncMode": {
    "y":     {   "SetMode":  "LFuncMode"           },
    "u":     { "Modifiers": ["LWin"]               },
    "i":     { "Modifiers": ["RControl"]           },
    "o":     { "Modifiers": ["RAlt"]               },
    "p":     { "Modifiers": ["RShift"]             },

    "h":     { "Modifiers": ["LWin"]               },
    "j":     {   "SetMode":  "LFuncMode"           },
    "k":     { "Modifiers": ["RControl"]           },
    "l":     { "Modifiers": ["RAlt"]               },
    ";":     { "Modifiers": ["RShift"]             },

    "1":     { "Action": "{f1}"                    },
    "2":     { "Action": "{f2}"                    },
    "3":     { "Action": "{f3}"                    },
    "4":     { "Action": "{f4}"                    },
    "5":     { "Action": "{f5}"                    },
    "6":     { "Action": "{f6}"                    }
},

"LModMode": {
    "h":     { "Modifiers": ["LWin"]               },
    "j":     {   "SetMode":  "LFuncMode"           },
    "k":     { "Modifiers": ["RControl"]           },
    "l":     { "Modifiers": ["RAlt"]               },
    ";":     { "Modifiers": ["RShift"]             }
},

"RPinkyMode": {
    "'":     { "Modifiers": ["RControl", "LWin"]   },
    "a":     { "ExecSub": "ToggleCapsLock" }
}
}
