{
"DefaultMode": {
    "a":        { "Action": "a", "SetMode": "LMode",  "Modifiers": ["LShift"]      },
    "o":        { "Action": "o", "SetMode": "LMode",  "Modifiers": ["LAlt"]        },
    "e":        { "Action": "e", "SetMode": "LMode",  "Modifiers": ["LControl"]    },
    "u":        { "Action": "u", "SetMode": "NavMode"                              },
    "i":        { "Action": "i", "SetMode": "LMode",  "Modifiers": ["LWin"]        },

    "d":        { "Action": "d", "SetMode": "RSpecial" },
    "h":        { "Action": "h", "SetMode": "RMode",  "Modifiers": ["LWin"]                  },
    "t":        { "Action": "t", "SetMode": "RMode",  "Modifiers": ["RControl"]              },
    "n":        { "Action": "n", "SetMode": "RMode",  "Modifiers": ["RAlt"]                  },
    "s":        { "Action": "s", "SetMode": "RMode",  "Modifiers": ["RShift"]                },
    "Dash":     { "Action": "-", "SetMode": "RMode",  "Modifiers": ["RControl", "LWin"]      }
},


"LMode": {
    "d":        { "Action": "d" },
    "h":        { "Action": "h" },
    "t":        { "Action": "t" },
    "n":        { "Action": "n" },
    "s":        { "Action": "s" },
    "Dash":     { "Action": "-" }
},


"RMode": {
    "a":        { "Action": "a" },
    "o":        { "Action": "o" },
    "e":        { "Action": "e" },
    "u":        { "Action": "u" },
    "i":        { "Action": "i" }
},


"NavMode": {
    "a":        { "SetMode": "NavMode",  "Modifiers": ["LShift"]       },
    "o":        { "SetMode": "NavMode",  "Modifiers": ["LAlt"]         },
    "e":        { "SetMode": "NavMode",  "Modifiers": ["LControl"]     },
    "i":        { "SetMode": "NavMode",  "Modifiers": ["LWin"]         },

    "7": {              "Action": "{f7}" },
