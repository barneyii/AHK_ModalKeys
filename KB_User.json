{
"DefaultMode": {
    "'":       { "SetMode": "RFuncMode"   },
    ",":       { "SetMode": "RFuncMode"   },
    ".":       { "SetMode": "RFuncMode"   },
    "p":       { "SetMode": "RFuncMode"   },

    "a":       { "SetMode": "RNavMode"   },
    "o":       { "SetMode": "RNavMode"   },
    "e":       { "SetMode": "RNavMode"   },
    "u":       { "SetMode": "RNavMode"   },
    "i":       { "SetMode": "RNavMode"   },

    "f":       { "SetMode": "LFuncMode"   },
    "g":       { "SetMode": "LFuncMode"   },
    "c":       { "SetMode": "LFuncMode"   },
    "r":       { "SetMode": "LFuncMode"   },
    "l":       { "SetMode": "LFuncMode"   },

    "d":       { "SetMode": "LModMode"    },
    "h":       { "SetMode": "LFuncMode"   },
    "t":       { "SetMode": "LModMode"    },
    "n":       { "SetMode": "LModMode"    },
    "s":       { "SetMode": "LModMode"    },
    "-":       { "SetMode": "LModMode"    }
},


"LFuncMode": {
    "f":     { "Modifiers": ["RWin"]               },
    "g":     { "SetMode":    "LFuncMode"           },
    "c":     { "Modifiers": ["RControl"]           },
    "r":     { "Modifiers": ["RAlt"]               },
    "l":     { "Modifiers": ["RShift"]             },

    "h":     {   "SetMode":  "LFuncMode"           },
    "t":     { "Modifiers": ["RControl"]           },
    "n":     { "Modifiers": ["RAlt"]               },
    "s":     { "Modifiers": ["RShift"]             },

    "1":     { "Action": "{f1}"                    },
    "2":     { "Action": "{f2}"                    },
    "3":     { "Action": "{f3}"                    },
    "4":     { "Action": "{f4}"                    },
    "5":     { "Action": "{f5}"                    },
    "6":     { "Action": "{f6}"                    }
},

"LModMode": {
    "d":     { "Modifiers": ["RWin"]               },
    "h":     {   "SetMode":  "LFuncMode"           },
    "t":     { "Modifiers": ["RControl"]           },
    "n":     { "Modifiers": ["RAlt"]               },
    "s":     { "Modifiers": ["RShift"]             },
    "-":     { "Modifiers": ["RControl", "LWin"]   }
},



"RFuncMode": {
    "'":    { "Modifiers": ["LShift"]    },
    ",":    { "Modifiers": ["LAlt"]      },
    ".":    { "Modifiers": ["LControl"]  },
    "p":    {   "SetMode":  "RFuncMode"  },
    "y":    { "Modifiers": ["LWin"]      },

    "7":    { "Action": "{f7}"           },
    "8":    { "Action": "{f8}"           },
    "9":    { "Action": "{f9}"           },
    "0":    { "Action": "{f10}"          },
    "[":    { "Action": "{f11}"          },
    "]":    { "Action": "{f12}"          }
},

"RNavMode": {
    "a":    { "Modifiers": ["LShift"]    },
    "o":    { "Modifiers": ["LAlt"]      },
    "e":    { "Modifiers": ["LControl"]  },
    "u":    {   "SetMode":  "RNavMode"   },
    "i":    { "Modifiers": ["LWin"]      },

    "f":    { "Action": "{Escape}"       },
    "g":    { "Action": "{Home}"         },
    "l":    { "Action": "{End}"          },
    "c":    { "Action": "{PgUp}"         },
    "r":    { "Action": "{PgDn}"         },
    "h":    { "Action": "{Left}"         },
    "s":    { "Action": "{Right}"        },
    "t":    { "Action": "{Up}"           },
    "n":    { "Action": "{Down}"         },
    "d":    { "Action": "{Backspace}"    },
    "-":    { "Action": "{Delete}"       },
    "b":    { "Action": "{x}"            }
}

}
