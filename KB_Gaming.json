{
"DefaultMode": {
    "`":     { "SetMode": "TildeMode"    },
    "'":     { "SetMode": "RPinkyMode"   },

    "e":    { "Action": "{Up}"    },
    "d":    { "Action": "{Down}"  },
    "s":    { "Action": "{Left}"  },
    "f":    { "Action": "{Right}" }
},

"Global": {
    "^e":   { "Send": "e"       },
    "^d":   { "Send": "d"       },
    "^s":   { "Send": "s"       },
    "^f":   { "Send": "f"       }
},

"TildeMode": {
    "`":       { "SetMode":  "TildeMode"     },
    "1":       { "Action": "{F1}"            },
    "2":       { "Action": "{F2}"            },
    "3":       { "Action": "{F3}"            },
    "4":       { "Action": "{F4}"            },
    "5":       { "Action": "{F5}"            },
    "6":       { "Action": "{F6}"            }
},

"RPinkyMode": {
    "'":     { "Modifiers": ["RControl", "LWin"]   },
    "a":     { "ExecSub": "ToggleCapsLock" }
}

}
